{
  "name": "react-test-scaffold",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel index.html",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress": "concurrently \"parcel index.html\" \"cypress run\"",
    "test": "jest",
    "coverage": "jest-coverage-ratchet && git add ./package.json && git diff-index --quiet HEAD || git commit -m \"update coverage threshold\"",
    "lint": "eslint src/**/*.js cypress/integration/**/*.js",
    "lint-fix": "eslint --fix src/**/*.js cypress/integration/**/*.js"
  },
  "gitHooks": {
    "pre-push": "npm run coverage",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js cypress/integration/**/*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^5.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/react": "^11.0.4",
    "babel-eslint": "^10.1.0",
    "cypress": "^5.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest": "^26.4.2",
    "jest-coverage-ratchet": "^0.2.3",
    "lint-staged": "^10.4.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "yorkie": "^2.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 100,
        "statements": 100,
        "functions": 100,
        "branches": 100
      }
    }
  }
}
